{
  "name": "@analytiq/docrouter-ts-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for DocRouter API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:unit": "TEST_TYPE=unit jest tests/unit src/__tests__",
    "test:integration": "TEST_TYPE=integration jest tests/integration",
    "test:all": "npm run test:unit && npm run test:integration",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "docrouter",
    "api",
    "sdk",
    "typescript",
    "document-processing",
    "ocr",
    "llm"
  ],
  "author": "Analytiq",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "bcrypt": "^5.1.1",
    "bson": "^6.3.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^6.3.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
