# To set up the docker-compose.yml environment,
# create a .env file in this folder and fill in the following variables
ENV="dev"
MONGODB_URI="mongodb://localhost:27017"

# FastAPI backend reachable from the nextjs frontend
NEXT_PUBLIC_API_URL="http://localhost:8000" 
# FastAPI backend reachable from the nextjs backend
API_URL="http://backend:8000" 
# NextJS frontend
NEXTAUTH_URL="http://localhost:3000"

NEXTAUTH_SECRET="your_nextauth_secret"
JWT_SECRET="your_jwt_secret"

# Github OAuth
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=

# Google OAuth
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=